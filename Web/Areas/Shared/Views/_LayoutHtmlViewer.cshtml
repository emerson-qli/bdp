@{
    var settingService = new Service.Setting.SettingService();
    var version = "1.0";
    try {
        version = typeof(Web.MvcApplication).Assembly.GetName().Version.ToString();
    } catch (Exception exception) {

    }
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Build & Dox - @ViewBag.Title</title>
    @RenderSection("Styles", required: false)
    <link href="~/Content/lib/sbadmin/sb-admin-2.css" rel="stylesheet" />
    <link href="~/Content/lib/font-awesome/css/all.css" rel="stylesheet" />
    <link href="~/Content/lib/amaran/amaran.css" rel="stylesheet" />
    <link href="~/Content/lib/selectize/selectize.bootstrap3.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.4.1/css/bootstrap-datepicker3.css" />
    <link href="~/Content/css/override.css" rel="stylesheet" />
</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">


        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">
                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
            <!-- End of Main Content -->

        </div>
        <!-- End of Content Wrapper -->

    </div>

    <script src="~/Content/lib/jquery/jquery.min.js"></script>
    <script src="~/Content/lib/amaran/amaran.js"></script>
    <script src="~/Content/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/lib/jquery/jquery.easing.js"></script>

    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.4.1/js/bootstrap-datepicker.min.js"></script>

    <script src="~/Content/lib/sbadmin/sb-admin-2.min.js"></script>
    <script src="~/Content/lib/moment/moment.js"></script>
    <script src="~/Content/js/common.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>

