                    <table class="table table-bordered" id="tbl-list" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th class="align-middle">
                                    @a.Name
                                </th>
                                <th class="w-5">
                                    <button class="btn btn-sm btn-outline-primary fa-pull-right btn-add-internal-issue" data-issue-category-id="@a.Id" data-issue-category-name="@a.Name"><i class="fa fa-plus"></i></button>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td colspan="2">
                                    <table class="table table-bordered" width="100%" cellspacing="0">
