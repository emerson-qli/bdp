
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Shared/Views/_Layout.cshtml";
    ViewBag.User = Model.Employee.Fullname;
}

@section Scripts{
    <script src="~/Content/lib/trumbowyg/trumbowyg.js"></script>
    <script src="~/Content/lib/datatable/jquery.dataTables.js"></script>
    <script src="~/Content/lib/datatable/dataTables.bootstrap4.js"></script>
    <script src="~/Content/js/organization/context/index.js"></script>

}

@model Web.Areas.Organization.Data.OrganizationViewModel

@section Styles{
    <link href="~/Content/lib/trumbowyg/trumbowyg.css" rel="stylesheet" />
    <link href="~/Content/css/locations/common.css" rel="stylesheet" />
}

<nav>
    <div class="nav nav-tabs f12" id="nav-tab" role="tablist">
        <a class="nav-item nav-link active show" id="nav-swot-tab" data-toggle="tab" href="#nav-swot" data-hash="#nav-swot" role="tab" aria-controls="nav-swot" aria-selected="true">SWOT Analysis</a>
        <a class="nav-item nav-link" id="nav-internal-tab" data-toggle="tab" href="#nav-internal" data-hash="#nav-internal" role="tab" aria-controls="nav-internal" aria-selected="false">Internal Issues</a>
