    </div>
</nav>
<div class="tab-content f12" id="nav-tabContent">
    <div class="tab-pane  fade in show active" id="nav-swot" role="tabpanel" aria-labelledby="nav-swot-tab">
        <div class="row no-gutters pt-2">
            <div class="col-6 pr-1">
                <table class="table table-bordered" id="tbl-list" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th class="align-middle">
                                Strength
                            </th>
                            <th class="w-5">
                                <button id="btn-add-strength" class="btn btn-sm btn-outline-primary fa-pull-right"><i class="fa fa-plus"></i></button>
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td colspan="2">
                                <table class="table table-bordered" width="100%" cellspacing="0">
                                    @foreach (var a in Model.Strengths) {
                                        <tr>
                                            <td class="w-50">
                                                @a.Name <br />
                                                <small>@a.IssueTypeName - @a.BusinessRiskTypeName</small>
                                            </td>
                                            <td>@a.EmployeeName</td>
                                            <td class="w-10 text-center"><div class="dv-box" style="background-color: @a.RatingColor;"></div></td>
                                            <td>@a.RatingName</td>
                                            <td class="w-10 text-center">
                                                <div class="btn-group" role="group">
                                                    <a href="javascript:void(0);" class="btn btn-sm btn-secondary btn-edit-swot" data-id="@a.Id" title="Edit"><i class="fa fa-edit"></i></a>
                                                    <a href="javascript:void(0);" class="btn btn-sm btn-danger btn-delete-swot" data-id="@a.Id" title="Delete"><i class="fa fa-trash"></i></a>
                                                </div>
                                            </td>
                                        </tr>
                                    }
                                </table>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
